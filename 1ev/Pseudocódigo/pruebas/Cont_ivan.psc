Algoritmo sin_titulo
	Cont<-0
	Repetir
		Cont<- Cont+1
		Escribir "Clave"
		Leer C
	Hasta Que C="mariamoliner" o Cont=3
	Si Cont = 3 y C<> "mariamoliner" Entonces
		Escribir "Numero de intentos maximos alcanzados"
		
	Fin Si
	Si C= "mariamoliner" Entonces
		Escribir "Contraseña Correcta"
	FinSi

FinAlgoritmo
